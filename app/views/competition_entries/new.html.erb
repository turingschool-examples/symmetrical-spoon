<h1>Register Your Team for <%= @competition.name %></h1>

<%= form_with url: competition_competition_entries_path(@competition.id), local: true do |f| %>
  <%= f.label :nickname %>
  <%= f.text_field :nickname %>
  <%= f.label :hometown %>
  <%= f.text_field :hometown %>
  <%= f.submit 'Register' %>
<% end %>