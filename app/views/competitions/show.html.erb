<h2><%= @competition.name %></h2>
<h3><%= @competition.location %></h3>
<h3><%= @competition.sport %></h3>

<h3>Teams</h3>
<% @competition.teams.each do |team| %>
    <p><%= team.nickname %></p>
    <p><%= team.hometown %></p>
<% end %>
<p>Competition Players Average Age: <%= @competition.average_players_age %></p><br>

<%= link_to "Register New Team", "/competitions/#{@competition.id}/new_team" %>
