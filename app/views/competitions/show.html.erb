<h1><%= @competition.name %></h1>
<p><%= @competition.location %></p>
<p><%= @competition.sport %></p>
<hr>
<h4>Average Age of Players: <%= @competition.players.average(:age) %></h4>
<hr>
<h3>Teams</h3>
<ul>
  <% @competition.teams.each do |t| %>
    <li><%= t.nickname.titleize %> | Hometown: <%= t.hometown %></li>
  <% end %>
</ul>
