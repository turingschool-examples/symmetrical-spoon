<h1>Fill Out New Team for Competition</h1>

<h3>
  <%= form_with url: "/competitions/#{@competition.id}", method: :post, local: true do |f| %>
    <%= f.label :hometown, "Hometown:" %> <%= f.text_field :hometown %><br/>
    <%= f.label :nickname, "Nickname:" %> <%= f.text_field :nickname %><br/>
    <%= f.submit "Submit" %>
  <% end %>
</h3>
