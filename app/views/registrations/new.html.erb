<h1> Register New Team <h1>
<%= form_with url: "/competitions/#{@competition.id}", local: true do |f| %>
  <%= f.hidden_field :competition_id, :value => @competition.id %>
  <%= f.label :nickname, "Nickname"%>
  <%= f.text_field :nickname %>
  <%= f.label :hometown, "Hometown"%>
  <%= f.text_field :hometown %>
  <%= f.submit "Register Team" %>
<% end %>